{
  "name": "luxon-angular",
  "version": "0.4.0",
  "description": "date pipes for Angular",
  "keywords": [
    "luxon",
    "angular",
    "date",
    "time",
    "datetime",
    "immutable"
  ],
  "homepage": "https://github.com/dstelljes/luxon-angular#readme",
  "bugs": "https://github.com/dstelljes/luxon-angular/issues",
  "repository": "github:dstelljes/luxon-angular",
  "license": "MIT",
  "contributors": [
    {
      "name": "Dan Stelljes",
      "email": "dan@stellj.es",
      "url": "https://stellj.es"
    }
  ],
  "main": "./library/index.js",
  "typings": "./library/index.d.ts",
  "devDependencies": {
    "@angular/common": "4.4.7",
    "@angular/compiler": "4.4.7",
    "@angular/compiler-cli": "4.4.7",
    "@angular/core": "4.4.7",
    "@angular/platform-browser": "4.4.7",
    "@angular/platform-browser-dynamic": "4.4.7",
    "@types/jest": "22.2.3",
    "@types/luxon": "0.5.2",
    "@types/node": "10.0.4",
    "jest": "22.4.3",
    "jest-zone-patch": "0.0.8",
    "luxon": "~1.2.0",
    "reflect-metadata": "0.1.12",
    "rxjs": "5.5.10",
    "typescript": "2.8.3",
    "zone.js": "0.8.26"
  },
  "peerDependencies": {
    "@angular/core": ">=2.0.0",
    "luxon": "~1.2.0"
  },
  "scripts": {
    "build": "ngc",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "roots": [
      "source"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/testing/setup.js",
    "testRegex": "\\.test\\.ts$",
    "transform": {
      "^.+\\.ts$": "<rootDir>/testing/transform.js"
    }
  }
