{
  "name": "luxon-angular",
  "version": "2.0.0",
  "description": "date pipes for Angular",
  "keywords": [
    "luxon",
    "angular",
    "date",
    "time",
    "datetime",
    "immutable"
  ],
  "homepage": "https://github.com/dstelljes/luxon-angular#readme",
  "bugs": "https://github.com/dstelljes/luxon-angular/issues",
  "repository": "github:dstelljes/luxon-angular",
  "license": "MIT",
  "contributors": [
    {
      "name": "Dan Stelljes",
      "email": "dan@stellj.es",
      "url": "https://stellj.es"
    }
  ],
  "main": "./cjs/index.js",
  "module": "./esm5/index.js",
  "es2015": "./esm2015/index.js",
  "typings": "./cjs/index.d.ts",
  "devDependencies": {
    "@angular/common": "8.0.0",
    "@angular/compiler": "8.0.0",
    "@angular/compiler-cli": "8.0.0",
    "@angular/core": "8.0.0",
    "@angular/platform-browser": "8.0.0",
    "@angular/platform-browser-dynamic": "8.0.0",
    "@types/jest": "24.0.13",
    "@types/luxon": "1.15.1",
    "@types/node": "12.0.7",
    "jest": "24.8.0",
    "jest-zone-patch": "0.0.10",
    "luxon": "1.16.0",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.3",
    "rxjs": "6.5.2",
    "tslint": "5.17.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.4.5",
    "zone.js": "0.9.1"
  },
  "peerDependencies": {
    "@angular/core": ">=6",
    "luxon": ">=1 <2"
  },
  "scripts": {
    "clean": "rimraf cjs esm5 esm2015",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "pretest": "npm run lint",
    "test": "jest",
    "prebuild": "run-s clean test",
    "build": "run-p build:*",
    "build:cjs": "ngc",
    "build:esm2015": "ngc --module es2015 --target es2015 --outDir esm2015",
    "build:esm5": "ngc --module es2015 --target es5 --outDir esm5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "roots": [
      "source"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/testing/setup.js"
    ],
    "testRegex": "\\.test\\.ts$",
    "transform": {
      "^.+\\.ts$": "<rootDir>/testing/transform.js"
    }
  }
}
