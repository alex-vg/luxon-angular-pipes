{
  "name": "luxon-angular",
  "version": "0.6.0",
  "description": "date pipes for Angular",
  "keywords": [
    "luxon",
    "angular",
    "date",
    "time",
    "datetime",
    "immutable"
  ],
  "homepage": "https://github.com/dstelljes/luxon-angular#readme",
  "bugs": "https://github.com/dstelljes/luxon-angular/issues",
  "repository": "github:dstelljes/luxon-angular",
  "license": "MIT",
  "contributors": [
    {
      "name": "Dan Stelljes",
      "email": "dan@stellj.es",
      "url": "https://stellj.es"
    }
  ],
  "main": "./library/index.js",
  "typings": "./library/index.d.ts",
  "devDependencies": {
    "@angular/common": "v4-lts",
    "@angular/compiler": "v4-lts",
    "@angular/compiler-cli": "v4-lts",
    "@angular/core": "v4-lts",
    "@angular/platform-browser": "v4-lts",
    "@angular/platform-browser-dynamic": "v4-lts",
    "@types/jest": "23.3.2",
    "@types/luxon": "1.2.2",
    "@types/node": "10.11.3",
    "jest": "23.6.0",
    "jest-zone-patch": "0.0.8",
    "luxon": "~1.4.0",
    "reflect-metadata": "0.1.12",
    "rxjs": "forward-compat",
    "typescript": "2.8.4",
    "zone.js": "0.8.26"
  },
  "peerDependencies": {
    "@angular/core": ">=2.0.0",
    "luxon": "~1.4.0"
  },
  "scripts": {
    "build": "ngc",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "roots": [
      "source"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/testing/setup.js",
    "testRegex": "\\.test\\.ts$",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.ts$": "<rootDir>/testing/transform.js"
    }
  }
}
