{
  "name": "luxon-angular",
  "version": "1.0.0",
  "description": "date pipes for Angular",
  "keywords": [
    "luxon",
    "angular",
    "date",
    "time",
    "datetime",
    "immutable"
  ],
  "homepage": "https://github.com/dstelljes/luxon-angular#readme",
  "bugs": "https://github.com/dstelljes/luxon-angular/issues",
  "repository": "github:dstelljes/luxon-angular",
  "license": "MIT",
  "contributors": [
    {
      "name": "Dan Stelljes",
      "email": "dan@stellj.es",
      "url": "https://stellj.es"
    }
  ],
  "main": "./library/index.js",
  "typings": "./library/index.d.ts",
  "devDependencies": {
    "@angular/common": "7.2.11",
    "@angular/compiler": "7.2.11",
    "@angular/compiler-cli": "7.2.11",
    "@angular/core": "7.2.11",
    "@angular/platform-browser": "7.2.11",
    "@angular/platform-browser-dynamic": "7.2.11",
    "@types/jest": "24.0.11",
    "@types/luxon": "1.11.1",
    "@types/node": "11.12.2",
    "jest": "24.5.0",
    "jest-zone-patch": "0.0.10",
    "luxon": "1.12.0",
    "rxjs": "6.4.0",
    "tslint": "5.14.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.2.4",
    "zone.js": "0.8.26"
  },
  "peerDependencies": {
    "@angular/core": ">=5",
    "luxon": ">=1 <2"
  },
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "pretest": "npm run lint",
    "test": "jest",
    "build": "ngc"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "roots": [
      "source"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/testing/setup.js"
    ],
    "testRegex": "\\.test\\.ts$",
    "transform": {
      "^.+\\.ts$": "<rootDir>/testing/transform.js"
    }
  }
}
